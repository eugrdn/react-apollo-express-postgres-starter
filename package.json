{
  "name": "app-starter",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3100/",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "pg": "^7.4.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "sequelize": "^4.35.2"
  },
  "scripts": {
    "develop:client": "react-scripts start",
    "develop:server": "nodemon ./src/server/index.js",
    "develop": "cross-env NODE_ENV=development npm-run-all --parallel develop:*",
    "prebuild": "rimraf ./build",
    "build": "react-scripts build",
    "start:server": "node ./src/server/index.js",
    "start": "cross-env NODE_ENV=production npm-run-all build start:server",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "dotenv": "^5.0.1",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.1",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2"
  }
}
